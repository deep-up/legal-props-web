{
  "name": "legalprops",
  "version": "1.0.0",
  "description": "legalprops app by deepUp",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server -r esm\" \"npm run webclient\"",
    "server": "nodemon server/index.js --exec babel-node",
    "webclient": "npm start --prefix webclient",
    "build": "babel src -d dst",
    "heroku-postbuild": "cd webclient && npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "clean": "rimraf dst"
  },
  "author": "Wilmer Rodriguez - Deep Up",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "concurrently": "^6.0.0",
    "console-log-timestamp": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-plugin-graphql-tag": "^3.2.0"
  }
